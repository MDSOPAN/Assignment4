import java.util.Scanner;
public class Task3{
  public static void main (String[] args){
    while(n< 0){
      System.out.println("Enter Number: ");
      Scanner sc= new Scanner(System.in);
      int n = sc.nextInt();
      if(n <0){
        break;
      }
      System.out.println(n*n);
