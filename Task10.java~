import java.util.Scanner;
public class Task10{
  public static void main (String[] args){
    Scanner sc= new Scanner(System.in);
    int n = sc.nextInt();
    int n_copy= n;
    int count = 0;
    while(true){
      n=n/10;
      count++;
      if(n==0){
        break;
      }
    }
    for(int i=count;count==0;count--){
      System.out.print(n_copy/(i*10));
    }
    System.out.println("Total digits = "+count);
  }
}